{
	"cli": {
		"version": ">= 3.0.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "development"
			},
			"channel": "development"
		},
		"preview": {
			"distribution": "internal",
			"channel": "preview",
			"android": {
				"buildType": "apk"
			}
		},
		"production": {
			"channel": "prod"
		}
	},
	"submit": {
		"production": {}
	}
}
